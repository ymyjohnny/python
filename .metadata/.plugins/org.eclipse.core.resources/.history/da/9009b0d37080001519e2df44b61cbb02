#!/usr/bin/python
#coding=utf-8
'''
Created on 2015-11-1
@author: ymy
'''

import re

def grep_a(filepath, regex):
    f = open(filepath)
    source = f.read()
    f.close()
    return list(regex.findall(source))

def main():
    regex = re.compile('<a[^>]*>' , re.MULTILINE)
    for m in grep_a('chardet.html',regex):
        print m
    
if __name__ == '__main__':
    main()

