from bs4 import BeautifulSoup


f = open('1643.html')
soup = f.read()

def get_urltitle(doc):
    url = doc.select('div#title')[0].get_text()
    print url



def main():
    get_urltitle(soup)

if __name__ == '__main__':
    main()