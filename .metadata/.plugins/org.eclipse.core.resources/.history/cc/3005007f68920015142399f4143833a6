import re
import sys


filter_file = open('filter_word.txt' )
#print filter_file.read()

def get_keywords(k):
    for i in k.splitlines():
        #print i
        yield i
        
def filter_file(s, regex):
    for word in regex:
        if re.search(word, s):
            s = s.replace(word, '**')
            #print s
    print s       
            
def main():
    s = sys.argv[1]
    #print s 
    k  = open('filter_word.txt' ).read()
    #print k
    regex = get_keywords(k)
    filter_file(s, regex)
    k.close()

if __name__ == '__main__':
    main()    