import random

def main():
    start = 1
    end = 1000
    number = random.randint(start, end+1)
    #print number
    in_number = raw_input("input (%d - %d) "  %(start, end))

    if in_number == 'quit':
        return
    in_number = int(in_number)
    if in_number == number:
        print "You win"
    if in_number > number:
        print 'too big'
        end = in_number
    if in_number < number:
        print 'too small'
        start = in_number
    
    

if __name__ == '__main__':
    main()