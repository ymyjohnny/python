import re
import os
import logging
import subprocess

def get_process(s):
      Process = subprocess.check_output(["ps","-ef"]).split_line()
      print Process
      for line in Process:
          if re.search(s):
              print line
      
def main():
    s = 'java'
    get_process(s)
    
    
if __name__ == '__main__':
    main()