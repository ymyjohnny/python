#使用os.*去操作一个不存在的文件(输入可以是数字么？)

import os

def test(a):
    try:
        for i in os.listdir(a):
            print i
    except Exception:
        return

def main():
    test(111111)
    
if __name__ == '__main__':
    main()
        
        