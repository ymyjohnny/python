import re
import os
import logging
from platform import java_ver

def get_process(s):
      Process = os.popen('ps' ,'-ef').read()
      print Process
      for line in Process:
          if re.search(s):
              print line
      
def main():
    s = 'java'
    get_process(s)
    
    
if __name__ == '__main__':
    main()