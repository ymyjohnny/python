import os

dbsize ={}
for root,dirs,files in os.walk('/home/ymy/workspace/pytest/'):
   # print root
   for file in files:
      filename = os.path.join(root,file)
      print filename
      
      fsize = os.stat(filename).st_size
      print fsize
      dbsize.append(filename)
      print dbsize
      
