import filecmp
import sys

try:
    dir1 = sys.argv[1]
    dir2 = sys.argv[2]
except:
    print 'Please input  diffdir.py dir1 dir2'
    sys.exit()

dirobject = filecmp.dircmp(dir1,dir2)
dirobject.report()