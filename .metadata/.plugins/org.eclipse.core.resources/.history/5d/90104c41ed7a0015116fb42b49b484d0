def abc(a,b,c):
    return c,b,a 
a,b,c = abc(1,2,3)
print a

#匿名函数
print (lambda a,b:a+b)(5,7)

##参数不固定用*
def add(*l):
    s = 0
    for i in l:
        s += i 
    return s 
print add(1,2,3,4,5,6,7)


def add1(a,b,c):
    print a,b,c 
add(1,b=2,c=3)














