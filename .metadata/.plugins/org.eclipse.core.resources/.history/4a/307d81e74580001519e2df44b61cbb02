#使用os.*去操作一个不存在的文件(输入可以是数字么？)

import os


def test(filename):
    try:
        os.open(filename)
            #raise test()
    except Exception:
        return 


def main():
    test('/tmp/111')
if __name__ == '__main__':
        main     