#猜数字，计算机有个数，人来猜

def main():
    start = 1
    end = 1001
    
    number = 666  
    i =  int(raw_input("input your number\n"))
    while True:
        
    #    current = (start + end) /2 
        if i > number:
            print  '%d is big'  % i 
            i = i-1
        elif i < number:
            print  '%d is small'  % i 
            i = i+1
        elif i == number:
            print '%d is zhe number' % i
            return
        
        
 #   print current


if __name__ == '__main__':
    main()
